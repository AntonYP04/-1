include <iostream>
using namespace std;

int main()
{
    int l=0,c=1;
    while(c!=0){
         cout << "Введите число:";
         cin >> c;
         for(int i=1;i<c;i++){
             
             if(c%i==0)l++;
        }
                if(l==1){
                cout << "Простое" << endl;
                }
    
    }
           
    return 0;
}
